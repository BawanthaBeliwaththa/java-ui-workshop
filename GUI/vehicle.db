CREATE TABLE vehicles (
    v_id INT AUTO_INCREMENT PRIMARY KEY,
    v_make VARCHAR(250) NOT NULL,
    v_model VARCHAR(250) NOT NULL,
    v_year INT NOT NULL,
    v_color VARCHAR(250),
    v_error_sheet VARCHAR(255) 
);

alter table vehicles AUTO_INCREMENT = 1000;

